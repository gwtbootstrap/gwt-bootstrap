<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="customcss.showcase">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.github.gwtbootstrap.Bootstrap" />
	<replace-with
		class="com.github.gwtbootstrap.showcase.client.resources.CustomConfigurator">
		<when-type-is
			class="com.github.gwtbootstrap.client.ui.config.Configurator" />
	</replace-with>
	<entry-point class='com.github.gwtbootstrap.showcase.client.Showcase' />
	<source path="client" />
</module>